#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("tinh tong hai so nguyen\n");
    int a,b;
    printf("nhap a:\n");
    scanf("%d",&a);
    printf("nhap b:\n");
    scanf("%d",&b);
    int tong=a+b;
    printf("tong cua a va b la: %d\n",tong);
    printf("\n");
    printf("pham_nhat_tan_s2_ex2\n");
    printf("Hello world!\n");
    return 0;
}
